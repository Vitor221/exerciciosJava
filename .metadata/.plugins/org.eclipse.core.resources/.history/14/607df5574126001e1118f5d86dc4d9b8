package entities;

class Employee {
	private Integer ID;
	private String name;
	private Double salary;
	
	public Employee(Integer iD, String name, Double salary) {
		ID = iD;
		this.name = name;
		this.salary = salary;
	}

	public Integer getID() {
		return ID;
	}

	public void setID(Integer iD) {
		ID = iD;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Double getSalary() {
		return salary;
	}
	
	
	
}
